<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" 
		elementFormDefault="qualified">
	<element name="data">
		<complexType>
			<sequence>
				<element name="environment">
					<complexType>
						<sequence>
							<element name="componentId" type="int" />
							<element name="screenName" type="string" />
							<element name="serverName" type="string" />
							<element name="assets" type="string" />
							<element name="contactName" type="string" />
							<element name="contactEmail" type="string" />
						</sequence>
					</complexType>
				</element>
				<element name="form" minOccurs="0" maxOccurs="unbounded">					
					<complexType>
						<sequence>
							<element name="id" type="long" />
							<element name="creationTimestamp" type="date" />
							<element name="startDate" type="date" />
							<element name="endDate" type="date" />
							<element name="type" type="string" />
							<element name="status" type="string" />
							<element name="title" type="string" />
							<element name="prettyUrl" type="string" />
							<element name="maxSubmissions" type="int" />
							<element name="submissionCount" type="int" />
							<element name="returnUrl" type="string" />
							<element name="skinUrl" type="string" />
							<element name="skinSelector" type="string" />
							<element name="question" minOccurs="0" maxOccurs="unbounded">												
								<complexType>
									<sequence>
										<element name="id" type="long" />
										<element name="number" type="int" />
										<element name="type" type="string" />
										<element name="header" type="string" />
										<element name="label" type="string" />
										<element name="page" type="int" />
										<element name="defaultAnswer" type="string" />
										<element name="filter" type="string" />
										<element name="maxCharacterLimit" type="int" />
										<element name="maxWordLimit" type="int" />
										<element name="required" type="boolean" />									
										<element name="possibleAnswer" minOccurs="0" maxOccurs="unbounded">					
											<complexType>
												<sequence>
													<element name="id" type="long" />
													<element name="label" type="string" />
													<element name="value" type="int" />
												</sequence>
											</complexType>
										</element>
										<element name="fkId" type="int" />
									</sequence>
								</complexType>
							</element>
							<element name="currentPage" type="int" />
							<element name="lastPage" type="int" />
							<element name="score" type="int" />
							<element name="messagePublicFormIntro" type="string" />
							<element name="messagePublicFormClosing" type="string" />
							<element name="messageEnded" type="string" />
							<element name="messageMaxSubmitted" type="string" />
							<element name="messageNotStarted" type="string" />
							<element name="messageOneSubmission" type="string" />
							<element name="messageThankYou" type="string" />
							<element name="started" type="boolean" />
							<element name="ended" type="boolean" />
							<element name="deleted" type="boolean" />
						</sequence>
					</complexType>
				</element>											
				<element name="possibleAnswer" minOccurs="0" maxOccurs="unbounded">					
					<complexType>
						<sequence>
							<element name="id" type="long" />
							<element name="label" type="string" />
							<element name="value" type="int" />
						</sequence>
					</complexType>
				</element>											
				<element name="score" minOccurs="0" maxOccurs="unbounded">					
					<complexType>
						<sequence>
							<element name="id" type="long" />
							<element name="begin" type="int" />
							<element name="end" type="int" />
							<element name="title" type="string" />
							<element name="summary" type="string" />
						</sequence>
					</complexType>
				</element>	
				<element name="submission" minOccurs="0" maxOccurs="unbounded">					
					<complexType>
						<sequence>
							<element name="id" type="long" />
							<element name="ip" type="string" />
							<element name="page" type="int" />
							<element name="userId" type="long" />
							<element name="answer" minOccurs="0" maxOccurs="unbounded">					
								<complexType>
									<sequence>
										<element name="questionId" type="long" />
										<element name="multipleChoice" type="boolean" />
										<element name="answerValue" type="string" />
										<element name="answerLabel" type="string" />
									</sequence>
								</complexType>
							</element>
						</sequence>
					</complexType>
				</element>				
				<element name="message" minOccurs="0" maxOccurs="unbounded">				
					<complexType>
						<sequence>
							<element name="type" type="string" />
							<element name="subtype" type="string" />
							<element name="label" type="string" />
							<element name="questionId" type="long" />
							<element name="questionNumber" type="int" />
							<element name="required" type="boolean" />
						</sequence>
					</complexType>				
				</element>
				<element name="user">
					<complexType>
						<sequence>
							<element name="id" type="long" />
							<element name="emailAddress" type="string" />
							<element name="firstName" type="string" />
							<element name="lastName" type="string" />
							<element name="pictureUrl" type="string" />
							<element name="active" type="boolean" />
							<element name="admin" type="boolean" />
						</sequence>
					</complexType>
				</element>
				<element name="analytics">
					<complexType>
						<sequence>
							<element name="startDateStr" type="string" />
							<element name="endDateStr" type="string" />
						</sequence>
					</complexType>
				</element>
			</sequence>
		</complexType>
	</element>
</schema>